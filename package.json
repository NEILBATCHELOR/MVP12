    "server:build": "tsc -p tsconfig.server.json",
    "server:dev": "ts-node-dev --respawn --transpile-only server.ts",
    "server": "node --experimental-specifier-resolution=node dist-server/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "build:all": "npm run build && npm run server:build",
    "build": "tsc ; vite build",
    "build-no-errors": "tsc ; vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
